    "tsc": "tsc",
    "postinstall": "npm run tsc",
    "build": "npm-run-all clean tsc",
    "copypublic": "copyfiles --flat src/public/**/* dist/public",
    "postbuild": "npm-run-all copypublic",
    "heroku-postbuild": "npm-run-all copypublic",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "pretest": "cross-env NODE_ENV=test npm run clean",
    "test": "cross-env NODE_ENV=test jest --runInBand --testTimeout=10000",
    "test:watch": "cross-env NODE_ENV=test jest --watch --testTimeout=10000"
